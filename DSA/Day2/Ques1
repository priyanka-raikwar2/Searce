#include <bits/stdc++.h>

using namespace std;

int main()
{
    stack<pair<int,int> > s, mx;
    int n, i=1;
    cin>>n;
    while(i<=n)
    {
        int key, val;
        pair<int, int> p1, p2;
        cin>>key;
        if(key == 1)
        {
            cin>>val;
            s.push(make_pair(val, i));
            if(mx.size() == 0)
            {
                mx.push(make_pair(val, i));
            }
            else
            {
                p1 = mx.top();
                if(p1.first < val)
                {
                    mx.push(make_pair(val, i));
                }
            }
        }
        else if(key == 2)
        {
            p1 = s.top();
            s.pop();
            p2 = mx.top();
            if(p1.first == p2.first && p1.second == p2.second)
            {
                mx.pop();
            }
        }
        else
        {
            p1 = mx.top();
            cout<<p1.first<<endl;
        }
        i++;
    }
    return 0;
}
