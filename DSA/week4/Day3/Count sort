#include<bits/stdc++.h>
using namespace std;

void counting_sort(int A[],int n,int B[],int k)
{
    int c[k+1],i,j;
    for(i=0; i<k+1; i++)                           //making an array of size k where k is the max element of given array and assigning it with 0
    {
        c[i]=0;
    }
    for(j=0; j<n; j++)                           // storing frequency of each element
    {
        c[A[j]]++;
    }
    for(i=1; i<k+1; i++)                           //updating array with the ending index of every distinct integer after sorting or finding commutative sum
        c[i]=c[i]+c[i-1];

    for(j=0; A[j]; j++)                            // final sorted array
    {
        B[c[A[j]]-1]=A[j];
        c[A[j]]--;
    }
}
int main()
{
int A[1000],n,k=INT_MIN,B[1000];
cin>>n;
for(int i=0;i<n;i++)
{
    cin>>A[i];
    if(k<A[i])
        k=A[i];
}
counting_sort(A,n,B,k);
for(int i=0;i<n;i++)
    cout<<B[i]<<" ";
return 0;

}
